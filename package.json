{
  "name": "@ialopezg/react-ui",
  "version": "0.0.0",
  "description": "Components & Helper Functions for ReactJS and NodeJS",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "unpkg": "dist/umd/bundle.min.js",
  "types": "dist/esm/index.d.ts",
  "sideEffects": [
    "*.css"
  ],
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "format": "prettier 'src/**/*.{js,ts,tsx}' --write",
    "lint": "eslint '**/*.{js,ts,tsx}'",
    "lint:fix": "npm run lint -- --fix",
    "prebuild": "rimraf dist",
    "prepare": "husky install",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:coveralls": "npm run test:coverage && coveralls < coverage/lcov.info",
    "test:staged": "jest --findRelatedTests ./src/**/*",
    "test:watch": "jest --watch",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build"
  },
  "keywords": [],
  "author": "Isidro A. Lopez G. <me@ialopezg> (https://ialopezg.com)",
  "repository": {
    "type": "git",
    "url": "https://github.com/ialopezg/react-ui.git"
  },
  "bugs": {
    "url": "https://github.com/ialopezg/react-ui/issues"
  },
  "homepage": "https://ialopezg.github.io/react-ui/",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.21.3",
    "@babel/plugin-transform-runtime": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.4",
    "@jest/globals": "^29.5.0",
    "@rollup/plugin-babel": "^6.0.3",
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^8.2.5",
    "@storybook/addon-essentials": "^7.0.6",
    "@storybook/addon-interactions": "^7.0.6",
    "@storybook/addon-links": "^7.0.6",
    "@storybook/blocks": "^7.0.6",
    "@storybook/react": "^7.0.6",
    "@storybook/react-webpack5": "^7.0.6",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
